/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=nots);
    Nand(a=a, b=nots, out=c1);
    Nand(a=b, b=sel, out=c2);
    Nand(a=c1, b=c2, out=out);
}
